<editors>

    <!-- IP Address Field -->
    <editor id="HIP">
        <range uom="56" min="0" max="254" step="1" prec="0" />
    </editor>

    <!-- Hub Connected Field -->
    <editor id="HCONNECT">
        <range uom="2" subset="0,1" />
    </editor>

    <!-- Color XY Value -->
    <editor id="HCLXY">
        <range uom="56" min="0" max="1" prec="4" />
    </editor>

    <!-- Color RGB Value -->
    <editor id="HCLRGB">
        <range uom="56" min="0" max="255" step="1" prec="0" />
    </editor>

    <!-- Brightness -->
    <editor id="HCLST">
        <range uom="51" min="0" max="100" prec="0" />
    </editor>

    <!-- Color Selector -->
    <editor id="HCSEL">
        <range uom="25" subset="1-24" nls="COLORS_HCSEL" />
    </editor>

</editors>
